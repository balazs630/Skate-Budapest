# vim: syntax=ruby
## Global platform
platform :ios, '10.0'

## For dynamic frameworks
use_frameworks!

## Inhibit all warnings
inhibit_all_warnings!

## Set the workspace
workspace 'SkateBudapest.xcworkspace'

## Inject pods
target 'SkateBudapest' do
  pod 'Alamofire', '~> 4.9.0'
  pod 'RealmSwift', '~> 3.18.0'
  pod 'IQKeyboardManagerSwift', '~> 6.2.1'
end
