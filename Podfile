# vim: syntax=ruby
## Global platform
platform :ios, '10.0'

## For dynamic frameworks
use_frameworks!

## Inhibit all warnings
inhibit_all_warnings!

## Set the workspace
workspace 'SkateBudapest.xcworkspace'

## Inject pods
target 'SkateBudapest' do
  pod 'Alamofire', '~> 4.8.0'
  pod 'RealmSwift', '~> 3.12.0'
end
